config $(ABI)_ABI_FLAGS
	int
	default $(flags)
	help
	  Flags for the $(ABI) ABI.

config $(ABI)_ABI_VER
	int
	default $(ver)
	help
	  The current or requested version for the $(ABI) ABI.
	  This config is used by both the provider and client of the ABI.

config $(ABI)_ABI_MAX_VER
	int
	default 255
	help
	  The maximum requested version for the $(ABI) ABI.
