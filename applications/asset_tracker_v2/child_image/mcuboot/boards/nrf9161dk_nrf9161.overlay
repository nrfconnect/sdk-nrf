/* Enable high drive mode for the SPI3 pins to get a square signal at 8 MHz */
&spi3_default {
	group1 {
		nordic,drive-mode = <NRF_DRIVE_H0H1>;
	};
};

&gd25lb256 {
	status = "okay";
};

/ {
	/* Configure partition manager to use gd25lb256 as the external flash */
	chosen {
		nordic,pm-ext-flash = &gd25lb256;
	};
};
