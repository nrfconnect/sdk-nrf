&uart1 {
	status = "okay";
	tx-pin = <20>;
	rx-pin = <22>;
	rts-pin = <19>;
	cts-pin = <21>;
};

&uart0 {
	rx-pin = <36>;
	tx-pin = <37>;
	/delete-property/ rts-pin;
	/delete-property/ cts-pin;
};
