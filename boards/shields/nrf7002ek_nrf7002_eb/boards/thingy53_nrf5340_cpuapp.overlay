 * This uses gpio0 pin 8 that conflicts with STATUS pin of Wi-Fi SR coex
 */
&npm1100_force_pwm_mode {
	status = "disabled";
};

/* Pins P0.9, P0.10, P0.11, P0.12 conflicting with SPI4,
 * nrf7002 host irq */
&uart0 {
	status = "disabled";
};
