&uart136 {
	hw-flow-control;
};

/* Extend by 160 KB, taken from rad */
&cpuapp_slot0_partition {
	reg = <0x40000 DT_SIZE_K(488)>;
};

/* Shrink by 160 KB */
/delete-node/ &cpurad_slot0_partition;
&mram1x {
	partitions {
		cpurad_slot0_partition: partition@BA000 {
			reg = <0xBA000 DT_SIZE_K(168)>;
		};
	};
};
