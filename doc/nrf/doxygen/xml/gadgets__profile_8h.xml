      <memberdef kind="function" id="group__bt__gadgets__profile_gac77612524e15cd32ca40a9d2169c3738_1gac77612524e15cd32ca40a9d2169c3738" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
