        <name>nfc_ndef_uri_msg_encode</name>
        <param>
          <type>enum <ref refid="group__nfc__uri__rec_gaf49d5f61e9463ac4e64651658a499595_1gaf49d5f61e9463ac4e64651658a499595" kindref="member">nfc_ndef_uri_rec_id</ref></type>
