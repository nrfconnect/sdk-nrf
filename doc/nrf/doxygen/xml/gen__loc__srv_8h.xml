        <name>bt_mesh_loc_srv_global_pub</name>
        <param>
          <type>struct <ref refid="structbt__mesh__loc__srv" kindref="compound">bt_mesh_loc_srv</ref> *</type>
          <declname>srv</declname>
        </param>
        <param>
          <type>struct bt_mesh_msg_ctx *</type>
          <declname>ctx</declname>
        </param>
        <param>
