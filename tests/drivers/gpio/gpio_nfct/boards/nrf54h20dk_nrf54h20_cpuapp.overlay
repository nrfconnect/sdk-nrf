/ {
	/* Test requirements:
	 * NFC antenna connected to the DK.
	 */
	test_gpios {
		compatible = "gpio-leds";
		out_gpios: out_gpios {
			gpios = <&gpio2 10 GPIO_ACTIVE_HIGH>;
		};

		in_gpios: in_gpios {
			gpios = <&gpio2 11 GPIO_ACTIVE_HIGH>;
		};
	};
};

&nfct {
	status = "okay";  /* not needed - APP is the default owner of NFCT */
};

&gpiote130 {
	status = "okay";
	owned-channels = <7>;
};

&gpio2 {
	status = "okay";
};
