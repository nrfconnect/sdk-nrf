# Redefinition to disable modemlib requirement from the tests as we want to mock it.
config NTN
	default y
	select AT_PARSER
	select AT_MONITOR
	imply CBPRINTF_FP_SUPPORT if LOG

menu "Zephyr Kernel"
source "Kconfig.zephyr"
endmenu
