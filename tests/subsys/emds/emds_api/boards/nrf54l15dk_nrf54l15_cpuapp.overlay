		};

		emds_partition_0: partition@163000 {
			label = "emds-0";
			reg = <0x00163000 0x00001000>;
		};

		emds_partition_1: partition@164000 {
			label = "emds-1";
			reg = <0x00164000 0x00001000>;
		};
	};
