/ {
	aliases {
		led = &led1;
	};

	leds {
		compatible = "gpio-leds";
		led1: led_1 {
			gpios = <&gpio9 1 GPIO_ACTIVE_HIGH>;
			label = "Green LED 1";
		};
	};
};

&gpio9 {
	status = "okay";
};

&gpiote130 {
	status = "okay";
};

&uart135 {
	status = "disabled";
	/delete-property/memory-regions;
};
