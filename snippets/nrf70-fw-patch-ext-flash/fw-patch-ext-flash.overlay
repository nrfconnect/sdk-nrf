&qspi {
	mx25r64: mx25r6435f@0 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;
		nrf70_fw_partition: partition@0 {
			label = "nrf70_fw_partition";
			reg = <0x0 DT_SIZE_K(128)>;
		};
	};
	};
};
