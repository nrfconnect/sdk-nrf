config PDN
	bool "PDN library interface for nRF9160"
	depends on BSD_LIBRARY && AT_CMD && AT_NOTIF

if PDN

config PDN_NUM_CONTEXTS
	int "Maximum number of allowed simutaneous PDN contexts"
	default 2


module=PDN
module-dep=LOG
module-str=PDN library
source "${ZEPHYR_BASE}/subsys/logging/Kconfig.template.log_config"

endif # PDN
